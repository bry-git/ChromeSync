{
  "short_name": "React App",
  "name": "Chrome Sync Chrome Extension",
  "description": "Open Source synchronization of Chrome data",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
      "default_popup": "index.html",
      "default_title": "Open the popup"
  },
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "permissions": [
    "tabs",
    "tabGroups",
    "storage",
    "bookmarks"
  ]
}
